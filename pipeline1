pipeline{
	agent any
	stages{
		stage('Compile Stage'){
		steps{
		withMaven(maven :  'maven') {
			sh  'mvn clean compile'
		}
	}
}

	stages{
		stage('Testing  Stage'){
		steps{
		withMaven(maven :  'maven') {
			sh  'mvn test'
 
		}
	}
}
		
	stages{
		stage('Deployment  Stage'){
		steps{
		withMaven(maven :  'maven') {
			sh  'mvn  deploy'
 
		}
	}
}
		
}
